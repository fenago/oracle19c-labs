DECLARE 
 v_my_record   user_credentials%ROWTYPE;
BEGIN
 v_my_record := check_login('ERNESTO','LEE');
 dbms_output.put_line('username	:'||v_my_record.user_name);
 dbms_output.put_line('password	:'||v_my_record.password);
EXCEPTION
 WHEN NO_DATA_FOUND THEN
  dbms_output.put_line('failed...');
END;
/

