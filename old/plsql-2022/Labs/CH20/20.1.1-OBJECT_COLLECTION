COLUMN TYPE_NAME FORMAT A20;
COLUMN ATTR_TYPE_NAME FORMAT A20;
COLUMN TABLE_NAME FORMAT A20;
COLUMN TABLE_TYPE FORMAT A20;

SELECT ATTR_NAME, ATTR_TYPE_MOD, ATTR_TYPE_NAME FROM USER_TYPE_ATTRS
WHERE TYPE_NAME='COMPANY'
/
SELECT method_name, method_type, parameters FROM USER_TYPE_METHODS
WHERE type_name='COMPANY'
/
SELECT table_name, table_type FROM USER_OBJECT_TABLES
/
