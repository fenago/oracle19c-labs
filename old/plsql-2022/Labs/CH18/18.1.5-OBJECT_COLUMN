DECLARE
 v_add   address := NEW address('','','','','');
BEGIN
 v_add.home      := 29;   
 v_add.street    := 'Payynkatu';
 v_add.zip_code  := 33700; 
 v_add.city      := 'Tampere';
 v_add.country   := 'Finland'; 
 INSERT INTO contacts(id,name,home_address, work_address)
 VALUES (1,'consultant',v_add, v_add);
 COMMIT;
END;
