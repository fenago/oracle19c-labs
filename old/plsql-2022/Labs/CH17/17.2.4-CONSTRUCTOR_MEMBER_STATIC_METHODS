CREATE OR REPLACE TYPE BODY car AS 
 CONSTRUCTOR FUNCTION car RETURN SELF AS RESULT IS
 BEGIN
  DBMS_OUTPUT.put_line('-- car()');
  RETURN;
 END;
 CONSTRUCTOR FUNCTION car(code NUMBER) 
 RETURN SELF AS RESULT IS
 BEGIN
  DBMS_OUTPUT.put_line('---- car(code)');
  SELF := car();
  SELF.code := code;
  RETURN;
 END;
 CONSTRUCTOR FUNCTION car(code     NUMBER,
                          model    VARCHAR2,
                          man_date DATE) 
 RETURN SELF AS RESULT IS
 BEGIN
  DBMS_OUTPUT.put_line('------ car(code,model,man_date)');
  SELF := car(code);
  SELF.model:= model;
  SELF.man_date:= man_date;
  RETURN;
 END;
END;
/
