CREATE OR REPLACE TYPE car AS OBJECT
(code     NUMBER,
 model    VARCHAR2(50),
 color    VARCHAR2(50),
 man_date DATE,
 MAP MEMBER FUNCTION year RETURN NUMBER
);
/

CREATE OR REPLACE TYPE BODY car AS 
 MAP MEMBER FUNCTION year RETURN NUMBER
 IS
 BEGIN
   RETURN TO_CHAR(man_date,'yyyy');
 END;
END;
/

