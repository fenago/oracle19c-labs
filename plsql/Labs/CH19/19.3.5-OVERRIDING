CREATE OR REPLACE TYPE BODY car AS
 CONSTRUCTOR FUNCTION car RETURN SELF AS RESULT IS
 BEGIN
   powertrain := '4x2';
   RETURN;
 END;
 OVERRIDING MEMBER FUNCTION details RETURN VARCHAR2 IS
 v_details  VARCHAR2(200);
 BEGIN
  v_details := (SELF AS vehicle).details;
  v_details := REPLACE(v_details,'Vehicle','Car');
  v_details :=  v_details||'powertrain   	:'||powertrain||chr(10);
  RETURN v_details;
 END; 
END;
/
