DECLARE
 v_mcycle   motorcycle := NEW motorcycle();
 v_car      car := NEW car(3366,'FORD','RED','01-FEB-99','4x4');
 v_vehicle  vehicle := NEW vehicle(1111,'JMC','BLACK','15-DEC-02');
BEGIN
 INSERT INTO vehicles 
 VALUES(v_mcycle);
 INSERT INTO vehicles 
 VALUES(v_car);
 INSERT INTO vehicles 
 VALUES(v_vehicle);
 COMMIT;
END;
/
